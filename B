	Callback = function()
		local Player = game.Players.LocalPlayer -- Reference to the local player
		local AnimationId = "13801090462" -- ID for the Think Emote
		local Anim = Instance.new("Animation") -- Create a new Animation instance
		Anim.AnimationId = "rbxassetid://" .. AnimationId -- Set the Animation ID
		local k = Player.Character.Humanoid:LoadAnimation(Anim) -- Load the animation onto the player's humanoid
		k:Play() -- Play the animation
		k:AdjustSpeed(getgenv().Multiplier or 1) -- Adjust the speed using the Multiplier
	end
})

